import static javatwo.Deck.makeDeck;


public class App {

    public static void main(String[] args) {


        ArrayList<Card> deck = makeDeck();

//        for (Card hand : deck) {
//            System.out.println(hand);
//        }

        Collections.shuffle(deck);

        for (int i=0; i<5; i++) {
            System.out.println(deck.get(i));
        }

    }
}



